@{
    ViewBag.Title = "MyCenter";
}
<link href="../Content/app/myCenter.css" rel="stylesheet" />
<div class="anchor_Top affix panel" data-spy="affix">
    <a class="anchor_linkTop" href="#all">
        <div class="anchor_titleTop" title="回顶部">
            <div class="fa fa-rocket"></div>
        </div>
    </a>
</div>
<div class="content_user">
    <div class="user_parent panel">
        <div class="user_l pull-left">
            <div class="user_ld pull-left">
                <img class="user_ld_img img-thumbnail" src="../Images/ceshi/pianpian.jpg" />
            </div>
            <div class="user_l_intro pull-left">
                <div class="user_li_ntl pull-left">
                    <div class="user_li_name">
                        <p>bain-片片</p>
                    </div>
                    <div class="user_li_title">
                        <p>插画师</p>
                    </div>
                    <div class="user_li_level">
                        <p>Lv.18</p>
                    </div>
                    <div class="user_li_site">
                        <p><em class="fa fa-map-marker"></em> china 江苏</p>
                    </div>
                </div>
                <div class="user_li_about pull-right">
                    <div class="user_lia_top clearfix">
                        <div class="user_liawaf pull-left">
                            <div class="user_liawaf_num"><p>999999</p></div>
                            <div class="user_liawaf_title"><p>作品</p></div>
                        </div>
                        <div class="user_liawaf pull-left">
                            <div class="user_liawaf_num"><p>999999</p></div>
                            <div class="user_liawaf_title"><p>关注</p></div>
                        </div>
                        <div class="user_liawaf user_liawaf_end pull-left">
                            <div class="user_liawaf_num"><p>9999999</p></div>
                            <div class="user_liawaf_title"><p>粉丝</p></div>
                        </div>
                    </div>
                    <div class="user_lia_bottom">
                        <button class="btn btn-success btn-xs pull-right" id="editInformation"><em class="fa fa-edit"></em> 修改资料</button>
                    </div>
                </div>
            </div>
            <div class="user_li_hi pull-left">
                <div class="user_lih_content">
                    <p><em class="fa fa-edit"></em> 插画师啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊啊</p>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>
<div class="mycenter">
    <div class="mycenter_left panel panel-default pull-left col-md-3">
        <div class="mycenter_tabs mycenter_news" id="mycenter_news">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-envelope"></span>&nbsp;&nbsp;消息中心</p></div>
                <div class="mycenter_tab_english"><p>Message center</p></div>
            </a>
        </div>
        <div class="mycenter_partition"></div>
        <div class="mycenter_tabs mycenter_newScrawlJottings" id="mycenter_newScrawlJottings">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-pencil"></span>&nbsp;&nbsp;新涂鸦/随笔</p></div>
                <div class="mycenter_tab_english"><p>Add new scrawl and jottings</p></div>
            </a>
        </div>
        <div class="mycenter_tabs mycenter_ScrawlJottings" id="mycenter_ScrawlJottings">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-book"></span>&nbsp;&nbsp;我的涂鸦/随笔</p></div>
                <div class="mycenter_tab_english"><p>My scrawl and jottings</p></div>
            </a>
        </div>
        <div class="mycenter_tabs mycenter_collectScrawlJottings" id="mycenter_collectScrawlJottings">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-th-large"></span>&nbsp;&nbsp;收藏的涂鸦/随笔</p></div>
                <div class="mycenter_tab_english"><p>Collection of the scrawl and jottings</p></div>
            </a>
        </div>
        <div class="mycenter_partition"></div>
        <div class="mycenter_tabs mycenter_attentionPerson" id="mycenter_attentionPerson">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-heart"></span>&nbsp;&nbsp;关注的人</p></div>
                <div class="mycenter_tab_english"><p>Attention of the people</p></div>
            </a>
        </div>
        <div class="mycenter_tabs mycenter_myFans" id="mycenter_myFans">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-leaf"></span>&nbsp;&nbsp;我的粉丝</p></div>
                <div class="mycenter_tab_english"><p>My Fans</p></div>
            </a>
        </div>
        <div class="mycenter_partition"></div>
        <div class="mycenter_tabs mycenter_editInformation" id="mycenter_editInformation">
            <a class="linkstyle" href="javascript:void(0)">
                <div class="mycenter_tab_china"><p><span class="fa fa-edit"></span>&nbsp;&nbsp;修改资料</p></div>
                <div class="mycenter_tab_english"><p>Modify personal information</p></div>
            </a>
        </div>
    </div>
    <div class="mycenter_right panel panel-default pull-right  col-md-9">
        @Html.Partial("NewScrawlAndJottings")
    </div>
    <div class="clearfix"></div>
</div>

<script src="../Scripts/app/mycenter.js"></script>


